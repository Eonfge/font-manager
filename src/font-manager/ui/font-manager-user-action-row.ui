<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.22"/>
  <template class="FontManagerUserActionRow" parent="GtkGrid">
    <property name="name">FontManagerUserActionRow</property>
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkEntry" id="action_name">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="margin_end">18</property>
        <property name="margin_top">18</property>
        <property name="margin_bottom">3</property>
        <property name="hexpand">True</property>
        <property name="placeholder_text" translatable="yes">Name</property>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="top_attach">1</property>
      </packing>
    </child>
    <child>
      <object class="GtkEntry" id="comment">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="margin_end">18</property>
        <property name="margin_top">3</property>
        <property name="margin_bottom">3</property>
        <property name="hexpand">True</property>
        <property name="placeholder_text" translatable="yes">Comment</property>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="top_attach">2</property>
      </packing>
    </child>
    <child>
      <object class="GtkEntry" id="executable">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="margin_end">18</property>
        <property name="margin_top">3</property>
        <property name="margin_bottom">3</property>
        <property name="hexpand">True</property>
        <property name="secondary_icon_name">folder-symbolic</property>
        <property name="placeholder_text" translatable="yes">Executable</property>
        <signal name="icon-press" handler="on_executable_icon_press" swapped="no"/>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="top_attach">3</property>
      </packing>
    </child>
    <child>
      <object class="GtkEntry" id="arguments">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="margin_end">18</property>
        <property name="margin_top">3</property>
        <property name="margin_bottom">18</property>
        <property name="hexpand">True</property>
        <property name="placeholder_text" translatable="yes">Arguments</property>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="top_attach">4</property>
      </packing>
    </child>
    <child>
      <object class="GtkImage" id="action_icon">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="margin_start">36</property>
        <property name="margin_end">36</property>
        <property name="margin_top">36</property>
        <property name="margin_bottom">36</property>
        <property name="pixel_size">64</property>
        <property name="icon_name">system-run-symbolic</property>
        <property name="icon_size">6</property>
      </object>
      <packing>
        <property name="left_attach">0</property>
        <property name="top_attach">1</property>
        <property name="height">4</property>
      </packing>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <style>
      <class name="view"/>
    </style>
  </template>
</interface>
